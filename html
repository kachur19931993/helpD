                                <div class="elementor-widget-container">
                                    <script>
                                        jQuery(function($) {
                                            let slides = $('#dental-logo-slider .swiper-slide');
                                            let arrows = $('#dental-logo-slider .swiper-button-next, #dental-logo-slider .swiper-button-prev');

                                            if ($(window).width() > 1024) {
                                                if (slides.length <= 4) {
                                                    arrows.css('display', 'none');
                                                }
                                            }

                                            $(window).resize(function() {
                                                if ($(window).width() > 1024) {
                                                    if (slides.length <= 4) {
                                                        arrows.css('display', 'none');
                                                    }
                                                } else {
                                                    arrows.css('display', 'flex');
                                                }
                                            })
                                        })
                                    </script>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
